{
  "name": "benz-audio-engine",
  "version": "0.2.10",
  "author": "Benz Leung",
  "description": "一个简单的基于 Web Audio API 的音效引擎",
  "keywords": [
    "web audio api"
  ],
  "scripts": {
    "uglify": "uglifyjs \"./benzAudioEngine.js\" -o \"./benzAudioEngine.min.js\"",
    "browserify": "browserify \"./src/\" > \"./benzAudioEngine.js\" -t [ babelify --presets [ env ] ] -s benzAudioEngine",
    "build": "npm run browserify && npm run uglify"
  },
  "homepage": "https://github.com/BenzLeung/benz-audio-engine",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "uglify-js": "^3.2.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/BenzLeung/benz-audio-engine.git"
  },
  "license": "MIT"
}
